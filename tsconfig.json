{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react"
  },
  "exclude": [
    "./node_modules",
    "./public",
    "./.cache",
    "./gatsby-config.ts",
    "./gatsby-node.ts",
    "gatsby-browser.js",
    "./gatsby-ssr.ts"
  ],
  "include": ["./src/**/*.tsx", "./src/**/*.ts"]
}
